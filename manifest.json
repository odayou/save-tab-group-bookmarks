{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "4.0",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "permissions": ["tabs", "bookmarks", "storage", "tabGroups", "contextMenus"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png",
      "512": "icon.png",
      "1024": "icon.png",
      "2048": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}